<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_calculate_type3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="@dimen/page_boarder_margin"
    android:orientation="vertical"
    android:visibility="gone">

    <RelativeLayout style="@style/OptionBar">

        <TextView
            android:id="@+id/lb_calculate"
            style="@style/TextView"
            android:text="@string/lb_calculate" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/lb_calculate"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <TextView
                style="@style/TextView.medium"

                android:background="@color/deepskyblue"
                android:text="@string/luckynumber2"
                android:textColor="@color/white" />

            <TextView

                style="@style/TextView.medium"
                android:text="= 数值" />

            <TextView
                android:id="@+id/a"
                style="@style/TextView.highlight"
                android:text="A"
                android:textSize="@dimen/text_medium" />


            <TextView
                style="@style/TextView.medium"
                android:text=" + " />

            <TextView
                style="@style/TextView.medium"
                android:text="10000001"
                android:textColor="@color/deepskyblue" />
        </LinearLayout>
    </RelativeLayout>

    <RelativeLayout
        style="@style/OptionBar"
        android:layout_marginTop="@dimen/page_boarder_margin">

        <TextView
            android:id="@+id/lb1"
            style="@style/TextView.medium"
            android:text="时时彩结果" />


        <TextView
            android:id="@+id/lb3"
            style="@style/TextView.medium"
            android:layout_toRightOf="@id/lb1"
            android:text="=" />

        <TextView
            android:id="@+id/calculate_lottery"
            style="@style/TextView.highlight"
            android:textSize="@dimen/text_medium"
            android:layout_marginBottom="@dimen/page_boarder_margin"
            android:layout_toRightOf="@id/lb3"
            android:gravity="left"
            android:text="等待开奖" />

        <TextView
            android:id="@+id/bt_gothirdpart"
            style="@style/TextView.link"
            android:textSize="13sp"
            android:text="开奖查询"
            android:clickable="true"
            android:onClick="onClick"
            android:layout_toRightOf="@id/calculate_lottery"
            android:layout_alignParentRight="true"
            android:gravity="right"
            />

        <TextView
            android:id="@+id/calculate_lottery_cycle"
            style="@style/TextView.highlight"
            android:layout_below="@id/calculate_lottery"
            android:layout_toRightOf="@id/lb1"
            android:gravity="left"
            android:text="@string/calculating_cycle"
            android:textSize="@dimen/text_medium" />


    </RelativeLayout>

    <RelativeLayout
        style="@style/OptionBar"
        android:layout_marginTop="@dimen/page_boarder_margin">

        <TextView
            android:id="@+id/lb6"
            style="@style/TextView.medium"
            android:text="数值" />

        <TextView
            android:id="@+id/lb7"
            style="@style/TextView.highlight"
            android:layout_toRightOf="@id/lb6"
            android:text="A"
            android:textSize="@dimen/text_medium" />

        <TextView
            android:id="@+id/lb8"
            style="@style/TextView.medium"
            android:layout_toRightOf="@id/lb7"
            android:text="=" />

        <TextView
            android:id="@+id/lb9"
            style="@style/TextView.medium"
            android:layout_marginBottom="@dimen/page_boarder_margin"
            android:layout_toRightOf="@id/lb8"
            android:gravity="left"
            android:text=" 时时彩开奖号码后" />
        <TextView
            android:id="@+id/lb13"
            style="@style/TextView.highlight"
            android:textSize="@dimen/text_medium"
            android:text="  ?  "
            android:layout_toRightOf="@id/lb9"
            />
        <TextView
            style="@style/TextView.medium"
            android:text="位"
            android:layout_toRightOf="@id/lb13"
            />

    </RelativeLayout>

    <RelativeLayout
        style="@style/OptionBar"
        android:layout_marginTop="2dp"
        android:layout_below="@id/lb9"
        >

        <TextView
            android:id="@+id/aaa_1"
            style="@style/TextView.medium"
            android:text="数值"
            android:visibility="invisible"/>

        <TextView
            android:id="@+id/aaa_2"
            style="@style/TextView.highlight"
            android:layout_toRightOf="@id/aaa_1"
            android:text="A"
            android:textSize="@dimen/text_medium"
            android:visibility="invisible"/>

        <TextView
            android:id="@+id/lb10"
            style="@style/TextView.medium"
            android:layout_toRightOf="@id/aaa_2"
            android:text="= " />

        <TextView
            android:id="@+id/calculate_A"
            style="@style/TextView.highlight"
            android:layout_alignBottom="@id/lb10"
            android:layout_toRightOf="@id/lb10"
            android:text="***********"
            android:textSize="16sp" />
    </RelativeLayout>

    <RelativeLayout
        style="@style/OptionBar"
        android:layout_marginTop="@dimen/page_boarder_margin">

        <TextView
            android:id="@+id/lb12"
            style="@style/TextView"
            android:text="计算结果" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@id/lb12"
            android:layout_centerHorizontal="true">


            <TextView
                style="@style/TextView.big"
                android:textStyle="bold"
                android:text="幸运号码："/>

            <TextView
                android:id="@+id/calculate_finalres"
                style="@style/TextView.highlight"
                android:text="******"
                android:textSize="@dimen/text_medium" />
        </LinearLayout>


    </RelativeLayout>

    <RelativeLayout
        style="@style/OptionBar.radio"
        android:layout_marginLeft="@dimen/page_boarder_margin"
        android:layout_marginRight="@dimen/page_boarder_margin"
        android:layout_marginTop="@dimen/page_boarder_margin">

        <!--<TextView
            android:id="@+id/lb14"
            style="@style/TextView.highlight"
            android:textSize="@dimen/text_medium"
            android:text="\?  "
            />-->
        <TextView
            android:id="@+id/lb15"
            style="@style/TextView.gray"
            android:text="说明："
            />

        <TextView
            style="@style/TextView.gray"
            android:layout_below="@id/lb15"
            android:text="@string/calculating_tips_2"
            android:gravity="left"
            />

    </RelativeLayout>

    <RelativeLayout
        style="@style/OptionBar.radio"
        android:layout_marginLeft="@dimen/page_boarder_margin"
        android:layout_marginRight="@dimen/page_boarder_margin"
        android:layout_marginTop="@dimen/page_boarder_margin">
        <TextView
            style="@style/TextView.gray"
            android:text="@string/cal_detail_type3"
            android:gravity="left"
            />

    </RelativeLayout>

    <!--<RelativeLayout
        style="@style/OptionBar.radio"
        android:layout_marginLeft="@dimen/page_boarder_margin"
        android:layout_marginRight="@dimen/page_boarder_margin"
        android:layout_marginTop="@dimen/page_boarder_margin">
        <TextView
            style="@style/TextView.gray"
            android:text="@string/calculating_tips_3"
            android:gravity="left"
            />

    </RelativeLayout>-->

</LinearLayout>